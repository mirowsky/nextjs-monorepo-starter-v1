{
  "npmScope": ".",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none",
        "export": true,
        "pascalCaseDirectory": true,
        "pascalCaseFiles": true
      },
      "storybook-configuration": {
        "configureCypress": false,
        "generateCypressSpecs": false,
        "generateStories": false,
        "linter": "eslint"
      },
      "library": {
        "style": "none",
        "linter": "eslint",
        "publishable": true,
        "buildable": true,
        "component": false,
        "compiler": "swc"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "core"
}
